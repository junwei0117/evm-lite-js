{
	"name": "evm-lite-core",
	"namespace": "mosaicnetworks",
	"author": "Danu Kumanan",
	"version": "1.0.0-alpha.1",
	"license": "MIT",
	"main": "./dist/index.js",
	"typings": "./dist/typings/index.d.ts",
	"scripts": {
		"dev": "npm install && npm run build && npm link",
		"clean": "rm -rf ./node_modules/ && rm -rf dist/ && rm -rf package-lock.json && rm -rf tsconfig.tsbuildinfo",
		"clean-yarn": "rm -rf ./node_modules/ && rm -rf yarn.lock && rm -rf dist/",
		"code": "ts-node tests/code.ts",
		"test": "jest",
		"package": "webpack --config config/webpack.config.js",
		"compile": "tsc -p ./tsconfig.json",
		"prepublish": "tsc -p ./tsconfig.json"
	},
	"dependencies": {
		"ascii-table": "^0.0.9",
		"browserify-cipher": "^1.0.1",
		"crypto-random-hex": "^1.0.0",
		"eth-lib": "^0.2.8",
		"json-bigint": "0.3.0",
		"keccak256": "^1.0.0",
		"randombytes": "^2.1.0",
		"scryptsy": "^2.0.0",
		"uuid": "^3.3.2",
		"web3": "0.20.6",
		"web3-eth-iban": "^1.0.0-beta.46",
		"web3-utils": "^1.0.0-beta.46"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mosaicnetworks/evm-lite-lib.git"
	},
	"keywords": [
		"evm",
		"evm-lite",
		"core",
		"mosaicnetworks",
		"evmljs"
	],
	"bugs": {
		"url": "https://github.com/mosaicnetworks/evm-lite-lib/issues"
	},
	"homepage": "https://github.com/mosaicnetworks/evm-lite-lib/tree/lerna/packages/core#readme",
	"resolutions": {
		"terser": "3.14.1"
	},
	"jest": {
		"testEnvironment": "node",
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	}
}
